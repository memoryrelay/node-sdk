{
  "name": "@memoryrelay/sdk",
  "version": "0.1.0",
  "description": "Official Node.js SDK for MemoryRelay - semantic memory storage for AI agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "memory",
    "ai",
    "agent",
    "semantic-search",
    "vector-database",
    "embeddings",
    "memoryrelay"
  ],
  "author": "MemoryRelay",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/memoryrelay/node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/memoryrelay/node-sdk/issues"
  },
  "homepage": "https://memoryrelay.ai",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "nock": "^13.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  }
}
